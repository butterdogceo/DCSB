<Application x:Class="DCSB.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Name="ThemeDictionary" Source="/DCSB.Icons;component/LightTheme.xaml" />
                <ResourceDictionary x:Name="ButtonIconDictionary" Source="/DCSB.Icons;component/Buttons.xaml" />
                <ResourceDictionary x:Name="VolumeIconDictionary" Source="/DCSB.Icons;component/Volume.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
